asrm.dev {
  log {
    output stdout
    format console
    level DEBUG
  }

  handle /clinicamv/api* {
    respond "API MATCH {uri}\n" 200
    # o simplemente: respond "API MATCH {uri}\n"
  }

  handle_path /clinicamv* {
    respond "FRONT MATCH {uri}\n" 200
  }

  respond "NO MATCH {uri}\n" 404
}
